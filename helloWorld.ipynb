    "import os, getpass\n",
    "def _set_env(var: str)\n",
    "    if not os.environ.get(var):\n",
    "        os.environ[var] = getpass.getpass(f\"Enter {var}: \")\n",
    "\n",
    "_set_env(\"API_KEY\")"
   ]
